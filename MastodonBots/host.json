{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information",
      "Host": "Warning",
      "Microsoft": "Warning",
      "Azure.Core": "Error",
      "Azure.Messaging.EventHubs": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}
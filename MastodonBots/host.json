{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information",
      "Host": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Azure.WebJobs": "Warning",
      "Azure.Core": "Error",
      "Azure.Messaging.EventHubs": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "Function.VeckaFunction": " Warning",
      "Function.VeckaFunction.User": " Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}